# RabbitMQ
RABBITMQ_HOST=127.0.0.1
RABBITMQ_PORT=5672
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_VHOST=/
RABBITMQ_EXPERIMENT_QUEUE=haitai.agent.benchmark.experiment

# Consumer
CONSUMER_CONCURRENT_CASES=2
CONSUMER_SCORER_CONCURRENT_CASES=2
CONSUMER_MAX_RETRIES=3
CONSUMER_CASE_TIMEOUT_SECONDS=180
CONSUMER_DOCKER_NETWORK=
CONSUMER_AGENT_EXEC_COMMAND=
CONSUMER_DOCKER_PULL_POLICY=if-not-present
CONSUMER_DOCKER_PULL_TIMEOUT_SECONDS=120
CONSUMER_DOCKER_RUN_TIMEOUT_SECONDS=60
CONSUMER_DOCKER_INSPECT_TIMEOUT_SECONDS=10

# OTel (consumer -> platform ingest)
CONSUMER_OTEL_ENABLED=false
CONSUMER_OTEL_ENDPOINT=
CONSUMER_OTEL_QUERY_TIMEOUT_SECONDS=10
CONSUMER_OTEL_PROTOCOL=http/protobuf
CONSUMER_OTEL_COLLECTOR_ENABLED=true
CONSUMER_OTEL_COLLECTOR_HOST=0.0.0.0
CONSUMER_OTEL_COLLECTOR_PORT=14318
CONSUMER_OTEL_COLLECTOR_PATH=/v1/traces
CONSUMER_OTEL_PUBLIC_ENDPOINT=http://host.docker.internal:14318/v1/traces

# Redis
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
REDIS_USERNAME=
REDIS_PASSWORD=
REDIS_DB=0
CONSUMER_REDIS_PROCESSING_LOCK_TTL_SECONDS=300
CONSUMER_REDIS_PROCESSED_TTL_SECONDS=86400

# Database
DATABASE_ENGINE=mysql
POSTGRES_SERVER=
POSTGRES_PORT=5432
POSTGRES_USER=
POSTGRES_PASSWORD=
POSTGRES_DB=
MYSQL_SERVER=127.0.0.1
MYSQL_PORT=3306
MYSQL_USER=root
MYSQL_PASSWORD=
MYSQL_DB=agent_benchmark

# Evaluator
CONSUMER_EVALUATOR_TIMEOUT_SECONDS=90
CONSUMER_EVALUATOR_CONNECT_TIMEOUT_SECONDS=15
CONSUMER_EVALUATOR_READ_TIMEOUT_SECONDS=90
CONSUMER_EVALUATOR_MAX_RETRIES=2
CONSUMER_EVALUATOR_RETRY_BACKOFF_SECONDS=1
CONSUMER_SCORER_HARD_TIMEOUT_SECONDS=120
